                </goals>
                <configuration>
                  <arguments>run build</arguments>
                  <nodeVersion>v12.13.1</nodeVersion>
                  <yarnVersion>v1.19.2</yarnVersion>
                </configuration>
              </execution>
            </executions>
            <configuration>
              <nodeVersion>v12.13.1</nodeVersion>
              <yarnVersion>v1.19.2</yarnVersion>
            </configuration>
          </plugin>
        </plugins>
      </build>
